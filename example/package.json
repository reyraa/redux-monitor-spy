    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "typescript": "^4.2.4"
  }
